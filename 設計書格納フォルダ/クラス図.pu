@startuml 発券機クラス図

class 発券機 {
    +購入画面へ移動()
    +商品を選択()
    +金額を入力()
    +おつりを受け取る()
    +購入を完了()
    +タイトル画面に戻る()
}

class 管理画面 {
    +商品一覧を表示()
    +総売上を表示()
    +管理メニューを表示()
    +管理コードを入力()
    +売上をリセット()
    +商品単価の変更()
}

class システム {
    +終了()
}

class 商品一覧 {
    +商品名
    +価格
}

発券機 "1" -- "1" 管理画面 : "アクセス"
発券機 "1" -- "1" システム : "制御"
発券機 "0..*" -- "1..*" 商品一覧 : "表示"
商品一覧 "1" -- "1" 管理画面 : "表示"

@enduml
